<Window xmlns="https://github.com/avaloniaui"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		x:Class="ImageFanReloaded.Controls.AboutWindow"
		Icon="/MainWindowIcon.ico"
		Title="About ImageFan Reloaded"
		Width="800"
		Height="600"
		WindowStartupLocation="CenterOwner"
		CanMinimize="False"
		CanResize="False"
		CanMaximize="False"
		Focusable="True"
		Loaded="OnWindowLoaded">
	<ScrollViewer Name="_aboutScrollViewer"
				  HorizontalScrollBarVisibility="Auto"
				  VerticalScrollBarVisibility="Auto"
				  Focusable="True">
		<SelectableTextBlock Name="_aboutTextBox"
				 Background="Transparent"
				 Margin="4"/>
	</ScrollViewer>
</Window>
